@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix ext: <http://example.org/ns/dcat-ap-ext#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ext:DatasetShape a sh:NodeShape ;
    sh:property [ sh:datatype xsd:string ;
            sh:description "A name given to the dataset."@en ;
            sh:minCount 1 ;
            sh:path dct:title ] ;
    sh:targetClass dcat:Dataset .

ext:HealthDatasetShape a sh:NodeShape ;
    sh:property [ sh:datatype xsd:integer ;
            sh:description "Number of patients in the dataset."@en ;
            sh:maxCount 1 ;
            sh:minCount 0 ;
            sh:path ext:patientCount ] ;
    sh:targetClass ext:HealthDataset .

